    <add key="ActiveDirectoryLDAPpath" value="LDAP://gkh.auto3.subaru.net/DC=gkh,DC=auto3,DC=subaru,DC=net" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <!-- サポートメールアドレス -->
    <add key="SupportMail" value="gkh-ejit-devplan@subaru.co.jp" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <!-- 旧版のアクセスパス -->
    <add key="oldDevPlanPath" value="https://gj1tds.subaru.co.jp/DevPlan3/DevPlan3Starter(検証).application" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
  </appSettings>
