<?xml version="1.0" encoding="utf-8" ?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings>
    <!--WebApiのURL-->
    <add key="WebApiBaseUrl" value="https://devplantestapi2.subaru.co.jp/" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <!--WebApiのパス-->
    <add key="WebApiBasePath" value="v1/" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <!-- ヘルプサイト -->
    <add key="HELP_SITE" value="https://gj1tdstest.subaru.co.jp/devplan/support/" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <!-- 駐車場PDFのURL -->
