<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <section name="DevPlan.Presentation.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
    </sectionGroup>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,log4net" />
    <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <section name="DevPlan.Presentation.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    </sectionGroup>
  </configSections>
  <!--アプリの設定-->
  <appSettings>
    <!--WebApiのURL-->
    <add key="WebApiBaseUrl" value="http://localhost" />
    <!--WebApiのパス-->
    <add key="WebApiBasePath" value="v2/" />
    <!--WebApiのタイムアウト(秒)-->
    <add key="WebApiTimeout" value="120" />
    <!--お気に入りの最大登録数-->
    <add key="FavoriteEntryMax" value="5" />
    <!--スケジュール項目の最大行数-->
    <add key="ScheduleItemRowMax" value="50" />
    <add key="ClientSettingsProvider.ServiceUri" value="" />
    <!-- 駐車場PDFのURL -->
    <add key="gunmaPdfFileUrl" value="https://gj1tds.subaru.co.jp/devplan/parking_map/g/所在地%20地図(群馬).pdf" />
    <add key="skcPdfFileUrl" value="https://gj1tds.subaru.co.jp/devplan/parking_map/skc/所在地%20地図(SKC).pdf" />
    <!-- MultiRow ヘッダー背景色 -->
    <add key="HADER_ROW_BACKCOLOR" value="Blue"/>
    <!-- MultiRow ヘッダー文字色 -->
    <add key="HADER_ROW_FORECOLOR" value="AliceBlue"/>
    <!-- MultiRow フィルター背景色 -->
    <add key="FILTER_ROW_BACKCOLOR" value="White"/>
    <!-- MultiRow フィルター文字色 -->
    <add key="FILTER_ROW_FORECOLOR" value="OrangeRed"/>
    <!-- MultiRow 一覧背景色 -->
    <add key="DATA_ROW_BACKCOLOR" value="Beige"/>
    <!-- MultiRow 一覧文字色 -->
    <add key="DATA_ROW_FORECOLOR" value="Black"/>
    <!-- MultiRow スクロール量 -->
    <add key="GRID_MOUSE_WHEEL_COUNT" value="15"/>
    <!-- CalendarGrid スクロール量 -->
    <add key="CalendarMouseWheelCount" value="20"/>
    <!-- ヘルプサイト -->
    <add key="HELP_SITE" value="https://gj1tds.subaru.co.jp/devplan/support/"/>
    <!-- ActiveDirectory アクセスパス -->
    <add key="ActiveDirectoryLDAPpath" value="LDAP://192.168.140.13/DC=sysexe-devplan,DC=test"/>
    <!-- サポートメールアドレス -->
    <add key="SupportMail" value="ta-ueda@system-exe.co.jp"/>
    <!--ログインセッションのタイムアウト(秒)-->
    <add key="LoginTimeout" value="14400" />
    <!-- Xeyeの全車両検索画面のパス -->
    <add key="XeyeSearchURL"  value="https://subaru.ol.ascy.jp/objectlocation/latestmap/index"/>
    <!-- Xeyeのログイン画面のパス -->
    <add key="XeyeLoginURL"  value="https://subaru.ol.ascy.jp/login/index"/>
    <!-- XeyeのTOP画面のパス -->
    <add key="XeyeTopURL"  value="https://subaru.ol.ascy.jp/objectlocation/latestlist/index"/>
    <!-- 旧版のアクセスパス -->
    <add key="oldDevPlanPath" value="http://gj1tds.gkh.auto3.subaru.net/DevPlan3/DevPlan3Starter.application"/>
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  <userSettings>
    <DevPlan.Presentation.Properties.Settings>
      <setting name="AutoLogin" serializeAs="String">
        <value>False</value>
      </setting>
      <setting name="UserID" serializeAs="String">
        <value />
      </setting>
      <setting name="Password" serializeAs="String">
        <value />
      </setting>
      <setting name="TestCarCalendarStyle" serializeAs="Xml">
        <value>
          <ArrayOfString xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <string>2</string>
            <string>0,0,1,9</string>
            <string>10,5,1,9</string>
            <string>0,0,1,9</string>
          </ArrayOfString>
        </value>
      </setting>
      <setting name="OuterCarCalendarStyle" serializeAs="Xml">
        <value>
          <ArrayOfString xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <string>3</string>
            <string>0,0,1,9</string>
            <string>10,5,1,9</string>
            <string>0,0,1,9</string>
          </ArrayOfString>
        </value>
      </setting>
      <setting name="CarShareCalendarStyle" serializeAs="Xml">
        <value>
          <ArrayOfString xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <string>3</string>
            <string>0,0,1,9</string>
            <string>10,5,1,9</string>
            <string>0,0,1,9</string>
          </ArrayOfString>
        </value>
      </setting>
      <setting name="TrackCalendarStyle" serializeAs="Xml">
        <value>
          <ArrayOfString xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <string>3</string>
            <string>0,0,1,9</string>
            <string>10,5,1,9</string>
            <string>0,40,1,9</string>
          </ArrayOfString>
        </value>
      </setting>
      <setting name="MyPendingCar" serializeAs="String">
        <value>True</value>
      </setting>
    </DevPlan.Presentation.Properties.Settings>
  </userSettings>
  <!-- ログ出力設定 -->
  <log4net>
    <appender name="LogFileAppender" type="log4net.Appender.RollingFileAppender">
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
      <param name="File" value="logs\application" />
      <param name="AppendToFile" value="true" />
      <param name="MaxSizeRollBackups" value="10" />
      <param name="MaximumFileSize" value="50MB" />
      <param name="RollingStyle" value="Composite" />
      <param name="StaticLogFileName" value="false" />
      <param name="DatePattern" value="&quot;.&quot;yyyyMMdd&quot;.log&quot;" />
      <layout type="log4net.Layout.PatternLayout">
        <param name="ConversionPattern" value="%d [%t] %1p (%C:%M:%L) - %m%n" />
      </layout>
    </appender>
    <root>
      <level value="DEBUG" />
      <appender-ref ref="LogFileAppender" />
    </root>
  </log4net>
  <applicationSettings>
    <DevPlan.Presentation.Properties.Settings>
      <setting name="HelpFile" serializeAs="String">
        <value>https://gj1tds.subaru.co.jp/devplan/support/</value>
      </setting>
    </DevPlan.Presentation.Properties.Settings>
  </applicationSettings>
  <system.web>
    <membership defaultProvider="ClientAuthenticationMembershipProvider">
      <providers>
        <add name="ClientAuthenticationMembershipProvider" type="System.Web.ClientServices.Providers.ClientFormsAuthenticationMembershipProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" />
      </providers>
    </membership>
    <roleManager defaultProvider="ClientRoleProvider" enabled="true">
      <providers>
        <add name="ClientRoleProvider" type="System.Web.ClientServices.Providers.ClientRoleProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" cacheTimeout="86400" />
      </providers>
    </roleManager>
  </system.web>
  <runtime>
    <assemblyBinding xmls="urn:schemas-microsoft-com:asm.v1">
      <probing privatePath="x86"/>
    </assemblyBinding>
  </runtime>
</configuration>